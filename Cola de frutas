fruits = []

# Pongamos en cola algunas frutas en nuestra lista
fruits.append('banana')
fruits.append('grapes')
fruits.append('mango')
fruits.append('orange')

# Ahora saquemos nuestras frutas, deber√≠amos obtener 'banana'
first_item = fruits.pop(0)
print(first_item)

# Si quitamos la cola de nuevo obtendremos 'uvas'
first_item = fruits.pop(0)
print(first_item)

# 'mango' y 'naranja' permanecen
print(fruits) # ['c', 'a']
